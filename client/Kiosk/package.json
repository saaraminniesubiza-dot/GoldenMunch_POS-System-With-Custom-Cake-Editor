    "@heroui/dropdown": "2.3.26",
    "@heroui/form": "2.1.26",
    "@heroui/framer-utils": "2.1.22",
    "@heroui/image": "2.2.16",
    "@heroui/input": "2.4.27",
    "@heroui/kbd": "2.2.21",
    "@heroui/link": "2.2.22",
    "@heroui/listbox": "2.3.25",
    "@heroui/modal": "2.2.23",
    "@heroui/navbar": "2.2.24",
    "@heroui/pagination": "2.2.23",
    "@heroui/popover": "2.3.26",
