{
  "name": "goldenmunch-kiosk",
  "version": "1.0.0",
  "description": "GoldenMunch POS Kiosk Application",
  "author": "GoldenMunch",
  "main": "electron/main.js",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack -p 3002",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --fix",
    "clean": "rm -rf node_modules package-lock.json",
    "clean:win": "if exist node_modules rmdir /s /q node_modules && if exist package-lock.json del package-lock.json",
    "fresh-install": "npm run clean && npm install",
    "fresh-install:win": "npm run clean:win && npm install",
    "install:safe": "npm install --fetch-timeout=300000 --fetch-retries=5",
    "install:electron": "npm install electron --save-dev --fetch-timeout=600000 --fetch-retries=10",
    "verify": "npm ls --depth=0 && npx electron --version",
    "troubleshoot": "echo 'See NPM_TROUBLESHOOTING.md for detailed help' && npm cache verify",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:3002 && electron .\"",
    "electron:build": "next build && electron-builder",
    "electron:build:win": "next build && electron-builder --win",
    "electron:build:mac": "next build && electron-builder --mac",
    "electron:build:linux": "next build && electron-builder --linux"
  },
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "@heroui/accordion": "2.2.23",
    "@heroui/alert": "2.2.26",
    "@heroui/autocomplete": "2.3.28",
    "@heroui/avatar": "2.2.21",
    "@heroui/badge": "2.2.16",
    "@heroui/button": "2.2.26",
    "@heroui/card": "2.2.24",
    "@heroui/chip": "2.2.21",
    "@heroui/code": "2.2.20",
